jQuery(document).ready(function(){	
function uc_ue_listing_carousel_elementor_954ce5a_start(){
		
  var objCarousel = jQuery('#uc_ue_listing_carousel_elementor_954ce5a');
  
  //show after js load
  objCarousel.show();
  
  		objCarousel.owlCarousel({
          		scrollToHead: false,
          		scrollToHeadOffset: 0,
                rtl:false,
                loop: true,
                center: false,
                autoplayTimeout: 5000,
                autoplayHoverPause: false,
                smartSpeed: 1500,
                navText: ["<i class=''></i>","<i class=''></i>"],
				setActiveClassOnMobile:true,
                changeItemOnClick:false,					
                nav: false,
                navigation:true,
                mouseDrag:true,
                touchDrag:true,  
                autoplay: false,
				margin: 20,
                preloadHeight: false,    
                autoHeight: false,     
                paddingType: "none",                 
                responsive : {
					0 : {
						items:1,
                                
                                
					},
					768 : {
						items:2,
                                
                                
					},
					980 : {
						items:3.6,
                                
                                 
					} 
			}
		  });
  
								
			objCarousel.trigger("uc-object-ready");
			jQuery("body").trigger("uc-remote-parent-init", [objCarousel]);
			  
		
        objCarousel.on("uc_ajax_sethtml",function(event, htmlItems, isAppend){

            objCarousel.trigger("replace.owl.carousel",htmlItems);
            objCarousel.trigger("refresh.owl.carousel");

        });
  	
}if(jQuery("#uc_ue_listing_carousel_elementor_954ce5a").length) uc_ue_listing_carousel_elementor_954ce5a_start(); else
	jQuery( document ).on( 'elementor/popup/show', () => { if(jQuery("#uc_ue_listing_carousel_elementor_954ce5a").length) uc_ue_listing_carousel_elementor_954ce5a_start();});
});


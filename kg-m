<!-- HFCM by 99 Robots - Snippet # 7: word limit -->
<script>
    document.addEventListener("DOMContentLoaded", function() {
        const limitChars = (selector, charLimit) => {
            const elements = document.querySelectorAll(selector);
            elements.forEach(element => {
                const text = element.innerText.trim(); // Trim whitespace
                if (text.length > charLimit) {
                    const limitedText = text.slice(0, charLimit) + "...";
                    element.innerText = limitedText;
                }
            });
        };

        limitChars('.custom-world-limit-post', 80);
    });
</script>

<!-- /end HFCM by 99 Robots -->
